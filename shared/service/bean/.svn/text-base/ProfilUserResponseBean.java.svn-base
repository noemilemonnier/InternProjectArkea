package com.arkea.com.arkea.commons.archetypes.shared.service.bean;

import java.io.Serializable;
import java.util.Map;

import com.arkea.com.arkea.commons.archetypes.shared.commons.Environnement;
import com.arkea.commons.ihm.client.ihmevent.model.ProfilSalarieBean;

/**
 * Descriptipon des donnees de sortie du service ProfilUserService
 */
@SuppressWarnings("serial")
public class ProfilUserResponseBean implements Serializable{

	private ProfilSalarieBean salarie;
	private Map<String, String> mapFederationsAuthorisee;
	private Environnement environnement;


	public ProfilSalarieBean getSalarie() {
		return salarie;
	}

	public void setSalarie(ProfilSalarieBean salarie) {
		this.salarie = salarie;
	}

	public Map<String, String> getMabFederationsAuthorisee() {
		return mapFederationsAuthorisee;
	}

	public void setMabFederationsAuthorisee(Map<String, String> tabFederationsAuthorisee) {
		this.mapFederationsAuthorisee = tabFederationsAuthorisee;
	}

	public Environnement getEnvironnement() {
		return environnement;
	}

	public void setEnvironnement(Environnement environnement) {
		this.environnement = environnement;
	}
}